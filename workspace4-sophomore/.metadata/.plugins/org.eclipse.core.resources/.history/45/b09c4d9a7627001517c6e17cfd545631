	ArrayList<HashMap<String, String>> dateList=new ArrayList<HashMap<String,String>>();
	for (int i = 0; i < 20; i++) {
		HashMap<String, String>map=new HashMap<String, String>();
		map.put("赛亚人"+i, i+"孙悟空");
		map.put("地球人"+i, i+"克林");
		map.put("那美克星人"+i, i+"比克");
		dateList.add(map);
	}
	for (int i = 0; i <20; i++) {
		System.out.println(dateList.get(i).get("赛亚人"+i));
