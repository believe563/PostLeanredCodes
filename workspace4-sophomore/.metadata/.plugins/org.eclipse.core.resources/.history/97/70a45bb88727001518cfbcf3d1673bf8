	//菱形?????
	for (int i = 0; i <5; i++) {
		for (int j = 0; j < 5; j++) {
			if (i==0||i==4) {
				if (j==5/2) {
					System.out.print("*");
				}else {
					System.out.print(" ");
				}
			}else if (i==5/2) {
				if (j==5/2) {
					System.out.println("*");
				}else {
					System.out.println(" ");
				}
			}else {
				if (j==i||j==5-1-i) {
					System.out.print("*");
				}else {
					System.out.print(" ");
				}
			}
		}
